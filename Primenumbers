#include<stdio.h>
#include<conio.h>
void main()
{
     int i,count=0,n;
     clrscr();
     printf("enter the number:");
     scanf ("%d",&n);
     for(i=2;i<n/2;i++)
     {
        if(n%i==0)
        {
           count++;
           break;
        }
     }
     if(count>0)
     {
     printf("the given number%d is not a prime number",n);
     }
     else
     {
          printf("the given number%d is a prime number",n);
     }
     getch();
     }
        
